{
    "name": "neha2601/facebook-cake-component",
    "type": "cakephp-plugin",
    "version": "1.0",
    "description": "Facebook APIs plugin",
    "keywords": ["cakephp","facebook", "api"],
    "homepage": "https://github.com/Neha2601/facebook-cake-component",
    "license": "MIT",
    "authors": [
        {
            "name": "nehal dobariya",
            "email": "nehal.dobariya@multidots.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.1.0 <4.0",
        "facebook/graph-sdk": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "FacebookApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
